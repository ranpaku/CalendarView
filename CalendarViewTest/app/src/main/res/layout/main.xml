 <TableRow android:longClickable="true"
     android:layout_width="fill_parent">
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#000000"
      android:layout_marginLeft="0pt"
      android:padding="1px">


   <TextView
       android:layout_width="30dp"
       android:layout_height="wrap_content"
       android:text="日"
       android:scrollbars="vertical"
       android:textSize="10pt"
       android:background="#ffffff"
       android:longClickable="false"
       android:layout_weight="5"
       android:allowUndo="true"
       android:layout_gravity="center_horizontal" />
  </LinearLayout>

  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#000000"
      android:padding="1px"

      android:weightSum="1">

   <TextView
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:text="会社名"
       android:textSize="10pt"
       android:background="#ffffff"
       android:layout_weight="30" />
  </LinearLayout>

  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#000000"
      android:padding="1px"
>
   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="場所"
       android:textSize="10pt"
       android:background="#ffffff"
       android:layout_weight="60.55" />
  </LinearLayout>

  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#000000"
      android:padding="1px"
   >
   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="目的"
       android:textSize="10pt"
       android:background="#ffffff"
       android:layout_weight="8.72" />
  </LinearLayout>

  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#000000"
      android:padding="1px"

      android:weightSum="1">
   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="時間"
       android:textSize="10pt"
       android:background="#ffffff"
       android:layout_weight="15" />
  </LinearLayout>
 </TableRow>



